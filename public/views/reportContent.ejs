        </div>
        <div class="filterRight">
                <span class="labelText">Hyresmodell:</span>
                <select name="rentModel" class="form-control form-select-sm" size="5" id="" multiple required>
                    <option value="all" <% if(locals.lastQuery){ if(locals.lastQuery.rentModel.includes('all') || locals.lastQuery.rentModel == "all"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >Alla</option>
                    <option value="N/A" <% if(locals.lastQuery){ if(locals.lastQuery.rentModel.includes('N/A') || locals.lastQuery.rentModel == "N/A"){  %> selected <% }} %> >N/A</option>
                    <option value="Nej" <% if(locals.lastQuery){ if(locals.lastQuery.rentModel.includes('Nej') || locals.lastQuery.rentModel == "Nej"){  %> selected <% }} %> >Nej</option>
                    <option value="Trio Rent" <% if(locals.lastQuery){ if(locals.lastQuery.rentModel.includes('Trio Rent') || locals.lastQuery.rentModel == "Trio Rent"){  %> selected <% }} %> >Trio Rent</option>
                    <option value="MLA" <% if(locals.lastQuery){ if(locals.lastQuery.rentModel.includes('MLA') || locals.lastQuery.rentModel == "MLA"){  %> selected <% }} %> >MLA</option>
                </select>      
                Funktionsavtal:
                <select name="funktionsavtal" class="form-control form-select-sm" id="" size="4" multiple required>
                    <option value="all" <% if(locals.lastQuery){ if(locals.lastQuery.funktionsavtal.includes('all') || locals.lastQuery.funktionsavtal == "all"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >Alla</option>
                    <option value="N/A" <% if(locals.lastQuery){ if(locals.lastQuery.funktionsavtal.includes('N/A') || locals.lastQuery.funktionsavtal == "N/A"){  %> selected <% }} %> >N/A</option>
                    <option value="Nej" <% if(locals.lastQuery){ if(locals.lastQuery.funktionsavtal.includes('Nej') || locals.lastQuery.funktionsavtal == "Nej"){  %> selected <% }} %> >Nej</option>
                    <option value="Ja" <% if(locals.lastQuery){ if(locals.lastQuery.funktionsavtal.includes('Ja') || locals.lastQuery.funktionsavtal == "Ja"){  %> selected <% }} %> >Ja</option>
                </select>
                <span class="labelText">Typ av lösning:</span>
                <select name="solution" id="" class="form-control form-select-sm" size="7" multiple required>
                    <option value="all" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('all') || locals.lastQuery.solution == "all"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >Alla</option>
                    <option value="Ej Angiven" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('Ej Angiven') || locals.lastQuery.solution == "Ej Angiven"){  %> selected <% }} %> >Ej Angiven</option>
                    <option value="TelcoCloud" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('TelcoCloud') || locals.lastQuery.solution == "TelcoCloud"){  %> selected <% }} %> >TelcoCloud</option>
                    <option value="Cisco HCS" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('Cisco HCS') || locals.lastQuery.solution == "Cisco HCS"){  %> selected <% }} %> >Cisco HCS</option>
                    <option value="Cygate Cloud" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('Cygate Cloud') || locals.lastQuery.solution == "Cygate Cloud"){  %> selected <% }} %> >Cygate Cloud</option>
                    <option value="CPE" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('CPE') || locals.lastQuery.solution == "CPE"){  %> selected <% }} %> >CPE</option>
                    <option value="TelcoCloud/Hybrid" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('TelcoCloud/Hybrid') || locals.lastQuery.solution == "TelcoCloud/Hybrid"){  %> selected <% }} %> >TelcoCloud/Hybrid</option>
                    <option value="Cygate CPE Hostad" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('Cygate CPE Hostad') || locals.lastQuery.solution == "Cygate CPE Hostad"){  %> selected <% }} %> >Cygate CPE Hostad</option>
                    <option value="Cygate CPE Hostad" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('CPE Hosted') || locals.lastQuery.solution == "CPE Hosted"){  %> selected <% }} %> >CPE Hosted</option>
                    <option value="Cygate CPE Hostad" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('CPE Main') || locals.lastQuery.solution == "CPE Main"){  %> selected <% }} %> >CPE Main</option>
                    <option value="Cygate CPE Hostad" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('CPE Branch') || locals.lastQuery.solution == "CPE Branch"){  %> selected <% }} %> >CPE Branch</option>
                    <option value="Cygate CPE Hostad" <% if(locals.lastQuery){ if(locals.lastQuery.solution.includes('Remote Site') || locals.lastQuery.solution == "Remote Site"){  %> selected <% }} %> >Remote Site</option>
                </select>
                <span class="labelText">Avslutat:</span>
                <select name="avslutatFilter" class="form-control">
                    <option value="all" <% if(locals.lastQuery){ if(locals.lastQuery.avslutatFilter == "all"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >Alla</option>
                    <option value="Ja" <% if(locals.lastQuery){ if(locals.lastQuery.avslutatFilter == "Ja"){  %> selected <% }} %> >Endast Avslutade</option>
                    <option value="Nej" <% if(locals.lastQuery){ if(locals.lastQuery.avslutatFilter == "Nej"){  %> selected <% }} %> >Ej Avslutade</option>
                </select>
        </div>
    </div>
  

<div>
    <button style="margin-bottom:40px;" type="submit" class="btn btn-lg btn-primary">Sök <i class="fas fa-search"></i></button>
</div>
</form>
</div>



