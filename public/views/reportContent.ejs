<div class="container containerMargin">
    <form method="POST" action="/searchReport">

    <h3>Sök och exportera data</h3>
    <div class="filterContainer">
        <div class="filterLeft">
            <div>
                <label>
                    <span class="labelText">Kundnamn:</span>
                    <input class="textBox" type="text" name="kundnamn" <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.kundnamn %>" <% } %> >
                </input>
                </label>
            </div>
            <div>
                <label>
                    <span class="labelText">Organisationsnummer:</span>
                    <input class ="textBox" type="text" name="orgnr" id="" pattern="[0-9]{6}-[0-9]{4}\s*|[0-9]{10}\s*" <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.orgnr %>" <% } %> >
                </label>
            </div>
            <div>
                <label>
                    <span class="labelText">Produkt:</span>
                    <input class="textBox" type="text" name="product" id="" <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.product %>" <% } %> >
                </label>
            </div>
            <div>
                <label>
                    <span class="labelText">Version: </span>
                    <select name="versionFilter">
                        <option value="lessThan" <% if(locals.lastQuery){ if(locals.lastQuery.versionFilter == "lessThan"){  %> selected <% }} %> ><</option>
                        <option value="lessThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.versionFilter == "lessThanEqual"){  %> selected <% }} %> ><=</option>
                        <option value="equal" <% if(locals.lastQuery){ if(locals.lastQuery.versionFilter == "equal"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >=</option>
                        <option value="greaterThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.versionFilter == "greaterThanEqual"){  %> selected <% }} %> >>=</option>
                        <option value="greaterThan" <% if(locals.lastQuery){ if(locals.lastQuery.versionFilter == "greaterThan"){  %> selected <% }} %> >></option>
                    </select>
                        <input class="textBox" type="text" name="version" id="" <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.version %>" <% } %> >
                </label>
            </div>
            <div>
                <label>
                    <span class="labelText">Licenser:</span>
                    <select name="licensesFilter">
                        <option value="lessThan" <% if(locals.lastQuery){ if(locals.lastQuery.licensesFilter == "lessThan"){  %> selected <% }} %> ><</option>
                        <option value="lessThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.licensesFilter == "lessThanEqual"){  %> selected <% }} %> ><=</option>
                        <option value="equal" <% if(locals.lastQuery){ if(locals.lastQuery.licensesFilter == "equal"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >=</option>
                        <option value="greaterThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.licensesFilter == "greaterThanEqual"){  %> selected <% }} %> >>=</option>
                        <option value="greaterThan" <% if(locals.lastQuery){ if(locals.lastQuery.licensesFilter == "greaterThan"){  %> selected <% }} %> >></option>
                    </select>
                    <input class="textBox" type="text" name="licenses" pattern="[0-9]+" <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.licenses %>" <% } %> >
                </label>
            </div>
            <div>
                <label>
                    <span class="labelText">Support köpt till:</span>
                    <select name="supportendFilter">
                        <option value="lessThan" <% if(locals.lastQuery){ if(locals.lastQuery.supportendFilter == "lessThan"){  %> selected <% }} %> ><</option>
                        <option value="lessThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.supportendFilter == "lessThanEqual"){  %> selected <% }} %> ><=</option>
                        <option value="equal" <% if(locals.lastQuery){ if(locals.lastQuery.supportendFilter == "equal"){  %> selected <% }}if(!locals.lastQuery){ %> selected <% } %> >=</option>
                        <option value="greaterThanEqual" <% if(locals.lastQuery){ if(locals.lastQuery.supportendFilter == "greaterThanEqual"){  %> selected <% }} %> >>=</option>
                        <option value="greaterThan" <% if(locals.lastQuery){ if(locals.lastQuery.supportendFilter == "greaterThan"){  %> selected <% }} %> >></option>
                </select>
                    <input class="textBox" type="date" name="supportend" id="supportend"  <% if(locals.lastQuery){ %> value="<%=locals.lastQuery.supportend %>" <% } %> >
                </label>
            </div>
